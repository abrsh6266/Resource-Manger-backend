else if depatment.Name == "" {
		c.JSON(http.StatusBadequest, gin.H{"eo": "invalid input"})
		etun
	}
else if (sting(val))[0:5] == "{\"e" {
		c.JSON(http.StatusBadequest, gin.H{"Eo": "invalid input"})
		etun
	}
else if (sting(val)) == "{\"data\":{\"Manage_by_pk\":null}}" {
